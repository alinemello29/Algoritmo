programa {
  funcao inicio() {
    //declaração da variaveis
    real dividendo
    real divisor
    real resultado

  //entrada de dados do usuoario dividendo e divisor
    escreva("Digite o dividendo")
    leia(dividendo)

    escreva("Digite o divisor")
    leia(divisor)
   
   //calculo da divisão e resultad 
    escreva(dividendo / divisor)
    escreva(resultado)
  }
}
